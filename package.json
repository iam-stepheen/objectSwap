{
  "name": "object-swap",
  "version": "0.0.1",
  "description": "",
  "author": "Okunade Stephen",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "trash dist coverage",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "postinstall": "npm run build",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "run-s migrate:up && node dist/main.js",
    "lintOld": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier -c 'src/**/*.{ts,js,json}'",
    "lint:tslint": "tslint --project .",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "fix:tslint": "tslint --project . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:create": "migrate create --template-file ./src/migrations-utils/template.ts --migrations-dir=\"./src/migrations\" --compiler=\"ts:./src/migrations-utils/ts-compiler.js\"",
    "migrate:up": "migrate --migrations-dir=\"./src/migrations\" --compiler=\"ts:./src/migrations-utils/ts-compiler.js\" up --store=\"./src/migrations-utils/mongoStore.ts\"",
    "migrate:down": "migrate --migrations-dir=\"./src/migrations\" --compiler=\"ts:./src/migrations-utils/ts-compiler.js\" down --store=\"./src/migrations-utils/mongoStore.ts\""
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/throttler": "^1.2.0",
    "chance": "^1.1.7",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "csurf": "^1.11.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "js-yaml": "^4.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "rxjs-compat": "^6.6.7",
    "slug": "^5.1.0",
    "swagger-ui-express": "^4.1.6",
    "trash-cli": "^4.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.18",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "esdoc-typescript-plugin": "^1.0.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.21.4",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
